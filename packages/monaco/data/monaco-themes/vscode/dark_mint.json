{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Mint - Midnight",
  "colors": {
    "editor.background": "#0e1924",
    "activityBar.background": "#0e1924",
    "sideBar.background": "#0e1924",
    "statusBar.foreground": "#0e1924",
    "statusBar.background": "#6fe08d",
    "tab.inactiveBackground": "#0e1924",
    "editorGroupHeader.tabsBackground": "#0e1924",
    "input.background": "#0e1924",
    "titleBar.activeBackground": "#0e1924",
    "titleBar.inactiveBackground": "#0e1924",
    "dropdown.background": "#0e1924",
    "editor.hoverHighlightBackground": "#40c0602c",
    "dropdown.border": "#6fe08d",
    "dropdown.listBackground": "#0e1924",
    "menu.background": "#0e1924",
    "quickInput.background": "#0e1924",
    "notificationCenter.border": "#6fe08d",
    "notificationCenterHeader.background": "#0e1924",
    "notificationToast.border": "#6fe08d",
    "notifications.background": "#0e1924",
    "editorGutter.background": "#0e1924",
    "tab.activeBackground": "#51c179",
    "tab.hoverBackground": "#40c0602c",
    "activityBarBadge.background": "#6fe08d",
    "button.background": "#6fe08d80",
    "button.hoverBackground": "#0f0f0f",
    "panelTitle.activeBorder": "#6fe08d",
    "input.border": "#6fe08d",
    "inputOption.activeBorder": "#6fe08d",
    "focusBorder": "#6fe08d",
    "editor.lineHighlightBorder": "#6fe08d",
    "list.hoverBackground": "#6fe08d80",
    "list.activeSelectionBackground": "#6fe08d80",
    "activityBar.foreground": "#6fe08d",
    "editor.foreground": "#79a9da",
    "sideBarTitle.foreground": "#79a9da",
    "editor.selectionBackground": "#6fe08d80",
    "window.activeBorder": "#6fe08d",
    "minimap.selectionHighlight": "#6fe08d80",
    "editorWhitespace.foreground": "#765D69",
    "editorIndentGuide.background": "#765D69",
    "editorIndentGuide.activeBackground": "#ccc",
    "scrollbar.shadow": "#00000000"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#636e72",
        "fontStyle": ""
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#b2bec3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "js template-expression",
      "scope": "punctuation.definition.template-expression.begin.js,punctuation.definition.template-expression.end.js,punctuation.definition.template-expression.begin.ts,punctuation.definition.template-expression.end.ts",
      "settings": {
        "foreground": "#833471"
      }
    },
    {
      "name": "TemplateString",
      "scope": "string.template.js",
      "settings": {
        "foreground": "#C4E538"
      }
    },
    {
      "name": "TemplateString",
      "scope": "meta.template.expression.js",
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#EE5A24"
      }
    },
    {
      "name": "Embeded String Begin and End",
      "scope": [
        "string.embedded.begin",
        "string.embedded.end"
      ],
      "settings": {
        "foreground": "#FFAB40"
      }
    },
    {
      "name": "Embeded String",
      "scope": "string.embedded",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#FFAB40"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FFAB40"
      }
    },
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#FFAB40",
        "fontStyle": ""
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.readwrite",
        "variable.readwrite.other.block"
      ],
      "settings": {
        "foreground": "#6F1E51"
      }
    },
    {
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "name": "Variables (local)",
      "scope": [
        "keyword.other.base",
        "keyword.other.this",
        "variable.other.object.property",
        "variable.language.base",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#40C4FF"
      }
    },
    {
      "name": "Variables (UnityEngine)",
      "scope": [
        "support.variable.UnityEngine"
      ],
      "settings": {
        "foreground": "#40C4FF"
      }
    },
    {
      "name": "Variables (UnityEditor)",
      "scope": [
        "support.variable.UnityEditor"
      ],
      "settings": {
        "foreground": "#40C4FF"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor"
      ],
      "settings": {
        "foreground": "#a50a90",
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#801a08",
        "fontStyle": ""
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#a06040",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#a06040"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#00e0e0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Other variable",
      "scope": [
        "variable.other.property",
        "variable.other.block"
      ],
      "settings": {
        "foreground": "#1B9CFC"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#2040c0"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "",
        "foreground": "#e0c040"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function-call",
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "Builtin Functions",
      "scope": [
        "function.support.builtin",
        "function.support.core"
      ],
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.class.js",
        "entity.name.tag.class.jsx"
      ],
      "settings": {
        "foreground": "#f5abd3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag Class and ID",
      "scope": [
        "entity.name.tag.class",
        "entity.name.tag.id"
      ],
      "settings": {
        "foreground": "#1e35ff"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#e0e000"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.variable"
      ],
      "settings": {
        "foreground": "#e06fc2"
      }
    },
    {
      "name": "Json Property",
      "scope": "support.dictionary.json",
      "settings": {
        "foreground": "#a06040"
      }
    },
    {
      "name": "JSON Key - Punctuation",
      "scope": [
        "source.json meta.structure.dictionary.json string.quoted.double.json punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#a06040"
      }
    },
    {
      "name": "JSON Key - String",
      "scope": "source.json string.quoted.double.json",
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#a06040"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "name": "JSON Key - Level 4",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#e06fc2"
      }
    },
    {
      "name": "JSON Key - Level 5",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FFAB40"
      }
    },
    {
      "name": "JSON Key - Level 6",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "name": "JSON Key - Level 7",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#40C4FF"
      }
    },
    {
      "name": "JSON Key - Level 8",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#e06fc2"
      }
    },
    {
      "name": "StyleSheet Property name",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.sass"
      ],
      "settings": {
        "foreground": "#61caff"
      }
    },
    {
      "name": "StyleSheet Property value",
      "scope": [
        "support.constant.css",
        "support.constant.scss",
        "support.constant.less",
        "support.constant.sass"
      ],
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "name": "StyleSheet Variable",
      "scope": [
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass"
      ],
      "settings": {
        "foreground": "#e06fc2"
      }
    },
    {
      "name": "StyleSheet Variable String",
      "scope": [
        "variable.css.string",
        "variable.scss.string",
        "variable.less.string",
        "variable.sass.string"
      ],
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "name": "StyleSheet Unit",
      "scope": [
        "unit.css",
        "unit.scss",
        "unit.less",
        "unit.sass"
      ],
      "settings": {
        "foreground": "#a06040"
      }
    },
    {
      "name": "StyleSheet Number",
      "scope": [
        "constant.numeric.css",
        "constant.numeric.scss",
        "constant.numeric.less",
        "constant.numeric.sass"
      ],
      "settings": {
        "foreground": "#ffa361"
      }
    },
    {
      "name": "StyleSheet Function",
      "scope": [
        "function.css",
        "function.scss",
        "function.less",
        "function.sass"
      ],
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#40C4FF"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#f8f8f0"
      }
    },
    {
      "name": "Link",
      "scope": "string.detected-link",
      "settings": {
        "foreground": "#40C4FF"
      }
    },
    {
      "name": "diff.header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
