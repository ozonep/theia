{
  "$schema": "vscode://schemas/color-theme",
  "name": "Real GitHub Dark",
  "colors": {
    "editor.background": "#0e1924",
    "activityBar.background": "#0e1924",
    "sideBar.background": "#0e1924",
    "statusBar.foreground": "#0e1924",
    "statusBar.background": "#6fe08d",
    "tab.inactiveBackground": "#0e1924",
    "tab.unfocusedActiveForeground": "#0e1924",
    "tab.activeBackground": "#51c179",
    "tab.hoverBackground": "#40c0602c",
    "editorGroupHeader.tabsBackground": "#0e1924",
    "input.background": "#0e1924",
    "titleBar.activeBackground": "#0e1924",
    "titleBar.inactiveBackground": "#0e1924",
    "dropdown.background": "#0e1924",
    "editor.hoverHighlightBackground": "#40c0602c",
    "dropdown.border": "#6fe08d",
    "dropdown.listBackground": "#0e1924",
    "menu.background": "#0e1924",
    "quickInput.background": "#0e1924",
    "notificationCenter.border": "#6fe08d",
    "notificationCenterHeader.background": "#0e1924",
    "notificationToast.border": "#6fe08d",
    "notifications.background": "#0e1924",
    "editorGutter.background": "#0e1924",
    "activityBarBadge.background": "#6fe08d",
    "button.background": "#6fe08d80",
    "button.hoverBackground": "#0f0f0f",
    "panelTitle.activeBorder": "#6fe08d",
    "input.border": "#6fe08d",
    "inputOption.activeBorder": "#6fe08d",
    "focusBorder": "#6fe08d",
    "editor.lineHighlightBorder": "#6fe08d",
    "list.hoverBackground": "#6fe08d80",
    "list.activeSelectionBackground": "#6fe08d80",
    "activityBar.foreground": "#6fe08d",
    "editor.foreground": "#79a9da",
    "sideBarTitle.foreground": "#79a9da",
    "editor.selectionBackground": "#6fe08d80",
    "window.activeBorder": "#6fe08d",
    "minimap.selectionHighlight": "#6fe08d80",
    "editorWhitespace.foreground": "#765D69",
    "editorIndentGuide.background": "#765D69",
    "editorIndentGuide.activeBackground": "#ccc",
    "scrollbar.shadow": "#00000000"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#8b949e"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language",
        "entity"
      ],
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": [
        "entity.name",
        "meta.export.default",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#ffa657"
      }
    },
    {
      "scope": [
        "variable.parameter.function",
        "meta.jsx.children",
        "meta.block",
        "meta.tag.attributes",
        "entity.name.constant",
        "meta.object.member",
        "meta.embedded.expression"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#e06fc2"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ffa657"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#ff7b72",
        "foreground": "#0d1117",
        "content": "^M"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "string source",
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7ee787"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#ffa657"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#490202",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#04260f",
        "foreground": "#7ee787"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "background": "#5a1e02",
        "foreground": "#ffa657"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#161b22",
        "background": "#6fe08d"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#e06fc2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#6fe08d"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#8b949e"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#ffa198"
      }
    },
    {
      "scope": [
        "constant.other.reference.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#a5d6ff",
        "fontStyle": "underline"
      }
    }
  ]
}
