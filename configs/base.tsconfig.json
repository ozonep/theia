{
    "compilerOptions": {
        "skipLibCheck": true,
        "declaration": true,
        "declarationMap": true,
        "noImplicitAny": false,
        "noEmitOnError": false,
        "noImplicitThis": true,
        "esModuleInterop": true,
        "noUnusedLocals": true,
        "strictNullChecks": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "downlevelIteration": true,
        "removeComments": true,
        "resolveJsonModule": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": false,
        "target": "es5",
        "jsx": "react",
        "lib": [
            "es6",
            "dom"
        ]
    }
}
